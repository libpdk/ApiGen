{layout '@layout.latte'}

{block content}
    <div id="container" style="width:100%;">
        <div class="uk-container uk-margin-small-top uk-margin-small-bottom phpapidoc-page-container phpapidoc-traits-page">
            <div class="phpapidoc-container uk-flex uk-flex-left">
                <div class="side-panel-container uk-visible@m">
                    {include "partial/api_catalog.latte"}
                </div>
                <div class="uk-width-expand phpapidoc-info-container phpapidoc-traits-info-container">
                    <div class="trait-entry">
                        <div class="title">
                            <img src="/statics/images/apidocs/php/trait.svg"/><h2>All Traits</h2>
                        </div>
                    </div>
                    {if count($traits)}
                        <ul>
                            {foreach $traits as $trait}
                                <li>
                                    <a href="{$trait|linkReflection}">{$trait->getName()}</a>
                                </li>
                            {/foreach}
                        </ul>
                    {/if}
                    <hr class="uk-divider-icon"/>
                    {include "partial/apigen_version.latte"}
                </div>
            </div>
        </div>
    </div>
{/block}
